#!/usr/bin/python3

import sys
file_names=sys.argv[1:]
print(file_names)

for  i in  file_names:
	f=open(i,'w')
	f.close()


#  it must not  replace any existing files 
# it must not  through any error for any existing content (file /folder)
#  if  object is present then update  current time not do not edit this file

